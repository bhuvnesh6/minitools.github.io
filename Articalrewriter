<script>
			$('#lang_drop_textarea_modes .item_box').click(function(){
		var _id=$(this).find('span').attr('id');
		if(_id != 'smart_spin'){
			$('#pp_alertmodal').modal('show');
			$('#pp_alertmodal p').text('This is free version. It does not support this mode');
		}
	})
	$('#lang_drop_textarea a').click(function(){
