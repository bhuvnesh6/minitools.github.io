	})
	var synth = window.speechSynthesis;
	synth.cancel();
	// hide function for social box
	function upload_section_hide(f){
		if($('#textbox').val().length == 0){
			$('.upload_option').slideUp(400);
			$('.step_1').addClass('active');
			$('#clear_icon_con').show();
			if(f){
				$('#textbox').focus();
			}
		}
	}
	//show function for social box
	function upload_section_show(){
		if($('#textbox').val().length == 0){
			$('.upload_option').slideDown(400);
			$('.step_1,.step_2,.step_3').removeClass('active');
			$('#clear_icon_con').hide();
		}
	}
	$('#clear_icon_con').click(function(){
		upload_section_show();
	})
	//textarea focusout for social box
	$('#textbox').focusout(function(){
		// $('.input_section').css('border-color','#eee');
		upload_section_show();
	});
	//textarea focus for social box
	$('#textbox').focus(function(e){
		e.stopPropagation();
		upload_section_hide(false);
	});
	//social box trigger only
	$('.upload_option').click(function(e){
		if(e.target !== e.currentTarget){
			if($(e.target).is('.d1,.d2,.d3,.d4')){
				upload_section_hide(true);
				return;
			}
		}
	});
	//Count Number of word inside text box
	function count(){
		setTimeout(function(){ 
			var val = $.trim($('#textbox').val());
			chars = val.length;
			if(!chars){
				words=0;
			}else{
				words = val.replace(/\s+/gi, ' ').split(' ').length;
			}
			$("#count_").html(words);
			// $("#count_,#count_res").html(words);
			//$("#count__char").html(chars);
		},500);
	}
	// file upload for getting file content
	$('#fileUpload').change(function () {
		var fileSelect = $('#fileUpload').prop('files')[0];
		var form_data = new FormData();
		f_name = fileSelect.name;
		if (fileSelect.size > 5000001 || fileSelect.size == undefined) {
			show_messages('error', 'Max File Size is 5mb');
			$('#fileUpload').val('');
			upload_section_show();
			sst_loader('',false);
			return false;
		}
		f_extension = f_name.substring(f_name.lastIndexOf('.') + 1);
		validExtensions = ["txt", "doc", "docx", "odt", "pdf", "rtf", "tex"];
		if ($.inArray(f_extension, validExtensions) != -1) {
			form_data.append('file_upload', fileSelect);
			File_upload(form_data,'textbox');
		} else {
			show_messages('error', 'Allowed file types .doc, .docx, .rtf, .odt, .pdf, .txt, tex');
			$('#fileUpload').val('');
			upload_section_show();
			sst_loader('',false);
			return false;
		}
	});
	//check submission validation
	function check_validation(){
		var text_val=$.trim($('#textbox').val());
		if(text_val == ''){
			show_messages('Error', 'No content found.');
			grecaptcha.reset();
		}else{
			var check_count=$('#count_').text();
			if(check_count == 0){
				show_messages('Error', 'No content found.');
				grecaptcha.reset();
			}else if(check_count < 15){
				show_messages('Error', 'Add at least 15 words to proceed.');
				grecaptcha.reset();
			}else if(check_count > 2000){
				$('#pp_alertmodal').modal('show');
				$('#pp_alertmodal p').text('This is free version. It does not support more then 2000 words.');
				// show_messages('Error', 'Words limit exceeded. 2000 words allowed per search.');
				grecaptcha.reset();
			}else{
				captcha_submit();
				sst_loader(Processing,true);
			}
		}
	}
	function _ondragenter(e){
    e.preventDefault(); 
		$('#textbox').css("background", "#cdcece");
	}
	function _ondragleave(e){
		e.preventDefault();
		$('#textbox').removeAttr("style");
	}
	function _ondragover(e){
		e.preventDefault();
		$('#textbox').css("background", "#cdcece");
	}
	function _ondrop(e){
		e.preventDefault();
		if (e.dataTransfer.items) {
			if(e.dataTransfer.items.length>1){
				show_messages('error', 'Drop Only one file');
				$('#textbox').removeAttr("style");
				return false;
			}else{
				for (var i = 0; i < 1; i++) {
					if (e.dataTransfer.items[i].kind === 'file') {
						var file = e.dataTransfer.items[i].getAsFile();
						var ext =  file.name.split('.').pop();
						var validImageTypes = ['.tex','.txt','.doc','.docx','.odt','.pdf','.rtf'];
						if ($.inArray('.'+ext, validImageTypes) < 0) {
							show_messages('error', 'Allowed file types .doc, .docx, .rtf, .odt, .pdf, .txt, tex');
							$('#textbox').removeAttr("style");
							return false;
						}
						if (parseInt(file.size) > 6*1024*1024) {
							show_messages('error', 'Max file siz is 5mb');
							$('#textbox').removeAttr("style");
							return false;
						}
						showLoader(true);
						var formData = new FormData();
						formData.append('file_upload',file);
						File_upload(formData,'textbox');
					}
				}
			}
		}
	}
	function File_upload(data_send,get_text_id){
		$.ajax({
			xhr: function() {
				var xhr = new window.XMLHttpRequest();
				xhr.upload.addEventListener("progress", function(evt) {
					if (evt.lengthComputable) {
						var percentage = evt.loaded / evt.total;
						percentage = parseInt(percentage * 100);
						var Loader_text=Uploading+percentage+'%';
						if(percentage == 100){
							var Loader_text=Processing;
						}
						sst_loader(Loader_text,true);
					}
				}, false);
				return xhr;
			},
			url: 'https://smallseotools.com/get-file-content/',
			type: 'post',
			processData: false,
			contentType: false,
			dataType: 'json',
			data: data_send,
			success: function (response) {
				sst_loader('',false);
				if (response.type == 'success') {
					upload_section_hide();
					$('#textbox').removeAttr("style");
					$("#textbox").val(response.msg);
					count();
				} else {
					upload_section_show();
					show_messages(response.type, response.msg);
					$('#fileUpload').val('');
					return false;
				}
			},
			error:function(){
				show_messages('error','File content not supported');
				sst_loader('',false);
			}
		});
	}
	// for text cut option 
	const source = document.querySelector('#textbox');
	source.addEventListener('cut', (event) => {
		count();
	});
	var base_url='https://smallseotools.com/';
</script>
