				function fixd_st(){
					if($('.sticky_ssb').length != 0){
						var win_height=$(window).scrollTop();
						var st_ofset_in=$('.sticky_ssb').offset().top;
						var st_ofset_out=$('.sticky_ofset_out').offset().top;
						if(win_height >= st_ofset_in){
							$('.sticky_ssb').css('position','fixed');
							$('.sticky_ssb').css('top','45px'); 
						}
						if(win_height <= st_ofset_out){
							$('.sticky_ssb').css('position','');
							$('.sticky_ssb').css('top','');
						}
						if($('.sticky_ssb').offset().top + $(".sticky_ssb").height()-100 > $(".main_area").height()){
							$('.sticky_ssb').css('top',-($(".sticky_ssb").offset().top + $(".sticky_ssb").height() - $(".sticky_stop").offset().top)-50);
						}
					}
				}
